package djuric;

public class Mejn {

	public static void main(String[] args) throws Exception {
		
		Autor.readTableAutori();
		
		Autor noviAutor = new Autor();
		
		System.out.println();
		
		noviAutor.create();
		
		System.out.println();
		
		noviAutor.read();
		
		Autor.readTableAutori();
		
		System.out.println();
		
		noviAutor.update();
		
		noviAutor.delete();
		
		Autor.readTableAutori();

	}
}
