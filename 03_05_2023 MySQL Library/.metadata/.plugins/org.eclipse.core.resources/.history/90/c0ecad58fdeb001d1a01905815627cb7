package djuric;

public class Biblioteke {
	
    private int bibliotekaID;
    private String naziv;
    private String mesto;
    private String ulica;
    private String broj;
    private int telefon;

    Biblioteke(int bibliotekaID, String naziv, String mesto, String ulica, String broj, int telefon) {
    	this.bibliotekaID = bibliotekaID;
        this.naziv = naziv;
        this.mesto = mesto;
        this.ulica = ulica;
        this.broj = broj;
        this.telefon = telefon;
    }

    public int getBibliotekaID() {
        return bibliotekaID;
    }

    public void setBibliotekaID(int bibliotekaID) {
        this.bibliotekaID = bibliotekaID;
    }

    public String getNaziv() {
        return naziv;
    }

    public void setNaziv(String naziv) {
        this.naziv = naziv;
    }

    public String getMesto() {
        return mesto;
    }

    public void setMesto(String mesto) {
        this.mesto = mesto;
    }

    public String getUlica() {
        return ulica;
    }

    public void setUlica(String ulica) {
        this.ulica = ulica;
    }

    public String getBroj() {
        return broj;
    }

    public void setBroj(String broj) {
        this.broj = broj;
    }

    public int getTelefon() {
        return telefon;
    }

    public void setTelefon(int telefon) {
        this.telefon = telefon;
    }
}
