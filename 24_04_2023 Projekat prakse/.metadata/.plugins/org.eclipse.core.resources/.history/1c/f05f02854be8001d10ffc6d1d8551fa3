package praksa;

public class Mejn {

	public static void main(String[] args) throws Exception {
		
		GoT got = new GoT();
		
		got.load();
		
		System.out.println("Daenerys` messages: ");
		
		Character daenerys = got.getCharacterByName("Daenerys");
		
		got.printCharacterMessages(daenerys);
		

		


	}	
}
