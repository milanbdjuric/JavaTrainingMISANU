package praksa;

public class Mejn {

	public static void main(String[] args) throws Exception {
		
		GoT got = new GoT();
		
		got.load();

		System.out.println(got.getCharacterDataList().get(0).getCharacter().getLastName());
		
		System.out.println(got.getCharacterMessages().get(got.getCharacterDataList().get(0).getCharacter()));
		

	}	
}
