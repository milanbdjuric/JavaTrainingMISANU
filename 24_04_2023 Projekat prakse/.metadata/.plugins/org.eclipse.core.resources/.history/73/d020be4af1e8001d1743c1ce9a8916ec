package praksa;

public enum EmojiType {
	
	HAPPY("happi", "(😄|🙂|😊|😍)"),
	SAD("sadd", "(😢|😭|😞|👿)"),
	LOVING("love", "(😍|😘)");
	
	private String mood, regex;

	EmojiType(String mood, String regex) {
		this.mood = mood;
		this.regex = regex;
	}

	public String getMood() {
		return mood;
	}

	public String getRegex() {
		return regex;
	}
}
