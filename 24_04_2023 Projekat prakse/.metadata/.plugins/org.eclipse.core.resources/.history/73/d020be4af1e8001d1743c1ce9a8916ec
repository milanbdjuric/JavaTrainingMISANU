package praksa;

public enum EmojiType {
	
	HAPPY("happi", "(ğŸ˜„|ğŸ™‚|ğŸ˜Š|ğŸ˜)"),
	SAD("sadd", "(ğŸ˜¢|ğŸ˜­|ğŸ˜|ğŸ‘¿)"),
	LOVING("love", "(ğŸ˜|ğŸ˜˜)");
	
	private String mood, regex;

	EmojiType(String mood, String regex) {
		this.mood = mood;
		this.regex = regex;
	}

	public String getMood() {
		return mood;
	}

	public String getRegex() {
		return regex;
	}
}
