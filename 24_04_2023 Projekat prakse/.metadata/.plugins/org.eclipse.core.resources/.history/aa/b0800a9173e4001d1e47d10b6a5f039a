package praksa;

public enum Emoji {
		
	HAPPY1("ğŸ˜„", "happy"),
	HAPPY2("ğŸ˜„", "happy"),
	HAPPY3("ğŸ™‚", "happy"),
	HAPPY4("ğŸ˜", "happy"),
	SAD1("ğŸ˜¢", "sad"),
	SAD2("ğŸ˜­", "sad"),
	SAD3("ğŸ‘¿", "sad"),
	SAD4("ğŸ˜", "sad");

	private final String emoji;
	private final String mood;
	
	Emoji(String emoji, String mood){
		this.emoji = emoji;
		this.mood = mood;
	}
	
	public String getEmoji() {
		return emoji;
	}

	public String getMood() {
		return mood;
	}
	
	
	
	

	
	
