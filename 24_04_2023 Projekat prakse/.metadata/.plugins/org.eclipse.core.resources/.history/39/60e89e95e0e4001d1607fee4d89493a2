package praksa;

public class EmojiMood {
	
	private String mood;

	EmojiMood(Emoji emoji){
		if(emoji.getEmoji().matches(".*[ğŸ˜„ğŸ˜ŠğŸ™‚ğŸ˜].*"))
			this.mood = "happy";
		if(emoji.getEmoji().matches(".*[ğŸ˜¢ğŸ˜­ğŸ‘¿ğŸ˜].*"))
			this.mood = "sad";
		if(emoji.getEmoji().matches(".*[ğŸ˜ğŸ˜˜].*"))
			this.mood = "love";
	}
	
	public String getMood() {
		return mood;
	}
	
	
	
	
	
	
	

	

	

	
	
}
