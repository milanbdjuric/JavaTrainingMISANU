package praksa;

import java.util.*;

public enum Emoji {
	
	LOVE("ğŸ˜˜"),
	HAPPY1("ğŸ˜„"),
	HAPPY2("ğŸ˜„"),
	HAPPY3("ğŸ™‚"),
	HAPPYLOVE("ğŸ˜"),
	SAD1("ğŸ˜¢"),
	SAD2("ğŸ˜­"),
	SAD3("ğŸ‘¿"),
	SAD4("ğŸ˜");

	private final String emoji;
	
	private static EmojiMood mood;
	
